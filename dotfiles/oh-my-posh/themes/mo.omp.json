{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "console_title_template": "{{ if .Env.CONSOLE_TITLE }}{{ .Env.CONSOLE_TITLE }}{{ else }}{{ .UserName }}@{{ .HostName }}:{{ .PWD }}{{ end }}",
  "palette": {
    "black": "#282828",
    "dark-gray": "#7c6f64",
    "gray": "#928374",
    "light-gray": "#a89984",
    "white": "#fbf1c7",

    "red": "#cc241d",
    "green": "#98971a",
    "yellow": "#d79921",
    "blue": "#458588",
    "purple": "#b16286",
    "aqua": "#689d6a",
    "orange": "#d65d0e",

    "light-red": "#fb4934",
    "light-green": "#b8bb26",
    "light-yellow": "#fabd2f",
    "light-blue": "#83a598",
    "light-purple": "#d3869b",
    "light-aqua": "#8ec07c",
    "light-orange": "#fe8019",

    "dark-red": "#9d0006",
    "dark-green": "#79740e",
    "dark-yellow": "#b57614",
    "dark-blue": "#076678",
    "dark-purple": "#8f3f71",
    "dark-aqua": "#427b58",
    "dark-orange": "#af3a03",
    

    "folder-bg": "p:dark-gray",
    "folder-fg": "p:light-yellow",

    "git-bg": "p:orange",
    "git-fg": "p:white",
    "git-clean": "p:light-green",
    "git-staged": "p:light-green",
    "git-conflicts": "p:red",
    "git-modified": "p:light-blue",
    "git-untracked": "p:gray",
    "git-stashed": "p:light-aqua",
    "git-ahead": "p:aqua",
    "git-behind": "p:aqua",
    "git-diverged": "p:dark-red",
    "git-branch": "p:white",
    "git-local": "p:yellow",
    "git-remote": "p:yellow",

    "git-staging": "p:light-green",
    "git-working": "p:light-red",

    "python-bg": "p:blue",
    "python-fg": "p:light-yellow",

    "username-bg": "p:yellow",
    "username-fg": "p:black",

    "filler-bg": "p:black",
    "filler-fg": "p:yellow",

    "time-bg": "p:light-purple",
    "time-fg": "p:black",

    "duration-bg": "p:light-aqua",
    "duration-fg": "p:black",

    "fail-bg": "p:light-red",
    "fail-fg": "p:dark-red",

    "exit-bg": "p:black",
    "exit-fg": "p:white"
  },
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "background": "p:black",
          "foreground": "p:yellow",
          "leading_diamond": "",
          "style": "diamond",
          "template": "┌─",
          "trailing_diamond": "",
          "type": "text"
        },
        {
          "background": "p:username-bg",
          "foreground": "p:username-fg",
          "style": "diamond",
          "leading_diamond": "",
          "template": "{{ .UserName }}@{{ .HostName }}",
          "trailing_diamond": "<p:yellow,p:dark-gray></>",
          "type": "session"
        },
        {
          "background": "p:folder-bg",
          "foreground": "p:folder-fg",
          "properties": {
            "style": "folder"
          },
          "style": "diamond",
          "leading_diamond": "",
          "template": "{{ path .PWD .Location }}",
          "trailing_diamond": " ",
          "type": "path"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "segments": [
        {
          "background": "p:git-bg",
          "foreground": "p:git-branch",
          "leading_diamond": "",
          "properties": {
            "fetch_status": true,
            "fetch_upstream_icon": true,

            "branch_icon": " ",
            "no_commits_icon": " ",
            "git_icon": "󰊢 "
          },
          "style": "diamond",
          "template": " {{ url .UpstreamIcon .UpstreamURL }} {{ .HEAD }}{{ .BranchStatus }} ",
          "type": "git"
        },
        {
          "background": "p:dark-green",
          "foreground": "p:white",
          "properties": {
            "fetch_status": true
          },
          "style": "powerline",
          "powerline_symbol": "",
          "template": " {{ .Staging.String }} ",
          "type": "git"
        },
        {
          "background": "p:dark-red",
          "foreground": "p:white",
          "properties": {
            "fetch_status": true
          },
          "style": "powerline",
          "powerline_symbol": "",
          "template": " {{ .Working.String }} ",
          "type": "git"
        },
        {
          "background": "p:git-bg",
          "background_templates": [
            "{{ if .IsWorkTree }}p:light-green{{ end }}"
          ],
          "foreground": "p:white",
          "foreground_templates": [
            "{{ if .IsWorkTree }}p:black{{ end }}"
          ],
          "properties": {
            "fetch_stash_count": true
          },
          "style": "diamond",
          "template": "{{ if gt .StashCount 0 }} 󰆓{{ .StashCount }}{{ end }}{{if gt .WorktreeCount 0 }} {{ .WorktreeCount }}{{ end }}\u2800󰊢 ",
          "trailing_diamond": " ",
          "type": "git"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "segments": [
        {
          "background": "p:python-bg",
          "foreground": "p:python-fg",
          "leading_diamond": "",
          "properties": {
            "fetch_version": false
          },
          "style": "diamond",
          "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }}",
          "trailing_diamond": " ",
          "type": "python"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "filler": "<p:filler-fg,p:filler-bg>─</>",
      "segments": [
        {
          "background": "p:fail-bg",
          "foreground": "p:fail-fg",
          "leading_diamond": " ",
          "style": "diamond",
          "template": "{{ if gt .Code 0 }} {{ .Code }} {{ reason .Code }}{{ end }}",
          "trailing_diamond": "",
          "type": "status"
        },
        {
          "background": "p:time-bg",
          "foreground": "p:time-fg",
          "foreground_templates": [
            "{{ if or (ge .CurrentDate.Hour 18) (lt .CurrentDate.Hour 6) }}p:light-yellow{{ else }}p:light-yellow{{ end }}"
          ],
          "background_templates": [
            "{{ if or (ge .CurrentDate.Hour 18) (lt .CurrentDate.Hour 6) }}p:dark-purple{{ else }}p:blue{{ end }}"
          ],
          "leading_diamond": " ",
          "properties": {
            "time_format": "03:04:05"
          },
          "style": "diamond",
          "template": " {{ .CurrentDate | date .Format }}",
          "trailing_diamond": "",
          "type": "time"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "background": "p:black",
          "foreground": "p:yellow",
          "style": "plain",
          "template": "└──",
          "type": "text"
        },
        {
          "background": "p:exit-bg",
          "foreground": "p:exit-fg",
          "foreground_templates": [
            "{{ if gt .Code 0 }}p:red{{ else }}p:green{{ end }}"
          ],
          "leading_diamond": "",
          "properties": {
            "always_enabled": true
          },
          "style": "diamond",
          "template": " {{ if gt .Code 0 }}{{ else }}{{ end }}",
          "trailing_diamond": "",
          "type": "status"
        },
        {
          "background": "p:exit-bg",
          "foreground": "p:exit-fg",
          "foreground_templates": [
            "{{ if gt .Code 0 }}p:purple{{ else }}p:aqua{{ end }}"
          ],
          "properties": {
            "always_enabled": true
          },
          "style": "plain",
          "template": "❯",
          "type": "status"
        },
        {
          "background": "p:black",
          "foreground": "p:blue",
          "style": "plain",
          "template": "❯",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "secondary_prompt": {
    "background": "p:black",
    "foreground": "p:yellow",
    "template": "└──  ⋮ ❯❯ "
  },
  "final_space": true,
  "version": 2
}
