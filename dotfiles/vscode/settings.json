        "#B16286",
        "#EBDBB2"
    ],
    "vim.statusBarColors.searchinprogressmode": [
        "#D79921",
        "#282828"
    ],
    "telemetry.telemetryLevel": "error",
    "scm.defaultViewMode": "tree",
    "vsicons.presets.jsOfficial": true,
    "vsicons.presets.tsOfficial": true,
    "vsicons.presets.jsonOfficial": false,
    "vsicons.presets.angular": true,
    "vsicons.presets.nestjs": true,
    "editor.linkedEditing": true,
    "editor.bracketPairColorization.enabled": true,
    "codesnap.backgroundColor": "#458588",
    "codesnap.boxShadow": "rgba(0, 0, 0, 0.55) 4px 6px 24px",
    "peacock.darkForegroundColor": "#282828",
    "peacock.lightForegroundColor": "#EBDBB2",
    "editor.cursorSurroundingLines": 999,
    "rest-client.enableTelemetry": false,
    "rest-client.previewResponsePanelTakeFocus": false,
    "rest-client.requestNameAsResponseTabTitle": true,
    "editor.guides.bracketPairs": true,
    "rest-client.previewResponseInUntitledDocument": true,
    "terminal.integrated.commandsToSkipShell": [
        "workbench.action.terminal.rename",
        "workbench.action.terminal.openDetectedLink",
        "workbench.action.toggleMaximizedPanel",
        "workbench.action.terminal.new",
        "workbench.action.createTerminalEditor",
        "workbench.action.terminal.split",
