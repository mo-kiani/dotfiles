    "workbench.tree.renderIndentGuides": "always",
    "workbench.tree.indent": 16,
    "gitlab.showPipelineUpdateNotifications": true,
    "excalidraw.theme": "auto",
    "remote.restoreForwardedPorts": false,
    "dotfiles.installCommand": "~/dotfiles/setup.sh ~/dotfiles/scripts/mysetup.sh",
    "dotfiles.repository": "https://github.com/mo-kiani/dotfiles.git",
    "settingsSync.ignoredSettings": [
        "-dotfiles.installCommand",
        "-dotfiles.repository",
        "-dotfiles.targetPath",
        "-dev.containers.copyGitConfig"
    ],
    "python.terminal.activateEnvironment": false,
    "search.useIgnoreFiles": false,
    "liveshare.guestApprovalRequired": true,
    "liveshare.showInStatusBar": "whileCollaborating",
    "peacock.vslsJoinColor": "#CC241D",
    "peacock.vslsShareColor": "#CC241D",
    "docker.containers.label": "ContainerName",
