    "window.title": "${rootName}${separator}${activeEditorMedium}",
    "window.restoreWindows": "preserve",
    "workbench.editor.highlightModifiedTabs": true,
    "terminal.integrated.profiles.windows": {
        "PowerShell": {
            "source": "PowerShell",
            "color": "terminal.ansiBlue",
            "icon": "terminal-powershell"
        },
        "Command Prompt": {
            "path": [
                "${env:windir}\\Sysnative\\cmd.exe",
                "${env:windir}\\System32\\cmd.exe"
            ],
            "args": [],
            "color": "terminal.ansiWhite",
            "icon": "terminal-cmd"
        },
        "Git Bash": {
            "source": "Git Bash",
            "color": "terminal.ansiMagenta",
            "icon": "terminal-bash"
        },
        "Ubuntu (WSL)": {
            "path": "C:\\Windows\\System32\\wsl.exe",
            "color": "terminal.ansiYellow",
            "icon": "terminal-ubuntu",
            "args": [
                "-d",
                "Ubuntu"
            ]
        }
    },
    "security.allowedUNCHosts": [
        "wsl.localhost"
    ],
    "editor.fontFamily": "SauceCodePro Nerd Font Mono, Consolas, 'Courier New', monospace",
    "workbench.iconTheme": "vscode-icons",
    "files.associations": {
        "**/.ssh/config": "ssh_config",
        "**/.ssh/private/config/*": "ssh_config"
    },
    "terminal.integrated.profiles.linux": {
        "bash": {
            "path": "bash",
            "color":"terminal.ansiYellow",
            "icon": "terminal-bash"
        },
        "tmux": {
            "path": "tmux",
            "color":"terminal.ansiGreen",
            "icon": "terminal-tmux"
        }
    },
    "terminal.integrated.tabs.title": "${sequence}",
    "terminal.integrated.fontFamily": "SauceCodePro Nerd Font Mono, Consolas, 'Courier New', monospace",
    "editor.stickyTabStops": true,
    "terminal.integrated.tabStopWidth": 4,
    "terminal.integrated.enableImages": true,
    "terminal.integrated.enableVisualBell": true,
    "terminal.integrated.bellDuration": 1200,
    "accessibility.signals.terminalBell": {
        "sound": "on"
    },
    "terminal.integrated.tabs.hideCondition": "never",
    "terminal.integrated.tabs.showActiveTerminal": "always",
    "terminal.integrated.tabs.showActions": "always",
    "files.simpleDialog.enable": true,
    "editor.rulers": [ 0, 80, 120 ],
    "workbench.activityBar.location": "top",
    "window.menuBarVisibility": "toggle",
    "terminal.external.windowsExec": "wt.exe",
    "codesnap.showWindowControls": false,
    "codesnap.transparentBackground": true,
    "window.newWindowDimensions": "maximized",
    "terminal.integrated.persistentSessionReviveProcess": "never",
    "terminal.integrated.confirmOnExit": "hasChildProcesses",
    "files.hotExit": "onExitAndWindowClose",
    "window.confirmBeforeClose": "keyboardOnly",
    "terminal.integrated.env.windows": {
        "WSLENV": "TERM_PROGRAM:${env:WSLENV}"
    },
    "peacock.affectSashHover": false,
    "peacock.affectStatusBar": false,
    "peacock.showColorInStatusBar": false,
    "peacock.affectActivityBar": false,
    "terminal.integrated.minimumContrastRatio": 1,
    "terminal.integrated.gpuAcceleration": "off",
    "terminal.integrated.customGlyphs": false,
    "terminal.integrated.confirmOnKill": "always",
    "terminal.integrated.showExitAlert": false,
    "terminal.integrated.shellIntegration.enabled": true,
    "terminal.integrated.showLinkHover": false,
    "terminal.integrated.smoothScrolling": true,
    "editor.quickSuggestions": {
        "other": "on",
        "comments": "on",
        "strings": "on"
    },
    "terminal.integrated.tabs.description": "${process} @ ${cwd}",
    "terminal.integrated.scrollback": 10000,
    "terminal.integrated.shellIntegration.history": 1000,
    "git.showCommitInput": false,
    "scm.showActionButton": false,
    "window.openFilesInNewWindow": "default",
    "gitlab.duoCodeSuggestions.enabled": false,
    "gitlab.duo.enabledWithoutGitlabProject": false,
    "gitlab.duoChat.enabled": false,
    "gitlab.duoCodeSuggestions.enabledSupportedLanguages": {
        "c": false,
        "cpp": false,
        "csharp": false,
        "go": false,
        "haml": false,
        "handlebars": false,
